CREATE TABLE Show (
    id TEXT NOT NULL,
    name TEXT NOT NULL,
    startDate TEXT NOT NULL,
    startTime TEXT NOT NULL,
    duration TEXT NOT NULL,
    genre TEXT NOT NULL,
    stage TEXT NOT NULL
);

insertShow:
INSERT INTO Show(id, name, startDate, startTime, duration, genre, stage)
VALUES(?, ?, ?, ?, ?, ?, ?);

removeAllShows:
DELETE FROM Show;

selectAllShows:
SELECT Show.*
FROM Show;